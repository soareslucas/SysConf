<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="confeccao"/>
  <table-filter match-name="aviamento"/>
  <table-filter match-name="modelo"/>
  <table-filter match-name="faccao"/>
  <table-filter match-name="faccao_has_peca"/>
  <table-filter match-name="modelo_has_corte"/>
  <table-filter match-name="corte"/>
  <table-filter match-name="faccionista"/>
  <table-filter match-name="faccao_has_aviamento"/>
  <table-filter match-name="peca"/>
</hibernate-reverse-engineering>
